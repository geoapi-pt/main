{
  "name": "geoapi.pt",
  "version": "1.0.3",
  "description": "Detect official divisional administrative regions of Portugal, providing coordinates",
  "scripts": {
    "test": "standard && depcheck --ignores=\"depcheck,leaflet\" --ignore-patterns=\"res/*\" && node server.js --testStartup && node test/mainTest.js",
